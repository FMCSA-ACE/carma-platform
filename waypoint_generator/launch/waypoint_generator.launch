<!-- 
Primary launch file for Waypoint Generator node
Loads parameters and configures logging for node, defaults to screen output.
 -->

<launch>
    <!-- Waypoint Generator Node -->
    <node pkg="waypoint_generator" type="node" name="waypoint_generator">
        <rosparam command="load" file="$(find waypoint_generator)/config/params.yaml" />
    </node>
</launch>
